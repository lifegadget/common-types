{
  "name": "common-types",
  "version": "1.31.1",
  "description": "Common Types for Typescript",
  "repository": "https://github.com/lifegadget/common-types",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "autoindex": "dd autoindex",
    "lint": "npx tsc --noEmit",
    "test": "npx tsc --noEmit",
    "clean": "rimraf dist",
    "prebuild": "run-s clean autoindex",
    "prepublish": "run-s clean autoindex build",
    "build": "tsup src/index.ts --format='cjs,esm' --dts",
    "watch": "run-s clean autoindex watch:bundle",
    "watch:bundle": "tsup src/index.ts --format='cjs,esm' --dts --watch"
  },
  "author": "Ken Snyder <ken@ken.net>",
  "license": "MIT",
  "devDependencies": {
    "@type-challenges/utils": "^0.1.1",
    "@types/node": "14",
    "@types/rimraf": "^3.0.2",
    "dd": "^0.18.0",
    "jest": "^27.4.7",
    "jest-extended": "^1.2.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "tsup": "^5.11.11",
    "typescript": "^4.5.4"
  }
}
